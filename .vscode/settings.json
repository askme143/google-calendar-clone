{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  }
}
